<!DOCTYPE xml>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>tvm_utility</name>
  <version>1.0.0</version>
  <description>
    A set of utility functions to help build an machine learning pipeline using
    TVM as the inference engine.
  </description>
  <maintainer email="liyou.zhou@arm.com">Liyou Zhou</maintainer>
  <license>Apache License 2.0</license>

  <build_depend>autoware_auto_cmake</build_depend>
  <build_depend>tvm_vendor</build_depend>

  <buildtool_export_depend>autoware_auto_cmake</buildtool_export_depend>

  <depend>libopenblas-dev</depend>
  <depend>libopencv-dev</depend>
  <depend>sensor_msgs</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
      <build_type>ament_cmake</build_type>
  </export>
</package>
