<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>object_collision_estimator_nodes</name>
  <version>1.0.0</version>
  <description>Detect possible collisions and modify local path to avoid them.</description>
  <maintainer email="liyou.zhou@arm.com">Liyou Zhou</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>
  <buildtool_depend>autoware_auto_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>object_collision_estimator</depend>
  <depend>autoware_auto_planning_msgs</depend>
  <depend>autoware_auto_tf2</depend>
  <depend>visualization_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>motion_common</depend>
  <depend>time_utils</depend>

  <test_depend>ament_cmake_gtest</test_depend>
  <test_depend>ament_index_python</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>
  <test_depend>ros_testing</test_depend>

  <build_depend>motion_testing</build_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
