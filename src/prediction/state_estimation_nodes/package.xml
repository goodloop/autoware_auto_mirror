<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>state_estimation_nodes</name>
    <version>1.0.0</version>
    <description>A node for state estimation that can be used in place of robot_localization ros package.</description>
    <maintainer email="opensource@apex.ai">Apex.AI, Inc.</maintainer>
    <license>Apache 2.0</license>

    <buildtool_depend>ament_cmake_auto</buildtool_depend>
    <buildtool_depend>autoware_auto_cmake</buildtool_depend>

    <depend>rclcpp</depend>
    <depend>rclcpp_components</depend>
    <depend>nav_msgs</depend>
    <depend>geometry_msgs</depend>
    <depend>state_vector</depend>
    <depend>motion_model</depend>
    <depend>kalman_filter</depend>
    <depend>autoware_auto_common</depend>
    <depend>tf2_geometry_msgs</depend>
    <depend>tf2_ros</depend>
    <depend>tf2</depend>
    <depend>mpark_variant_vendor</depend>

    <test_depend>ament_lint_auto</test_depend>
    <test_depend>ament_lint_common</test_depend>
    <test_depend>ament_cmake_gtest</test_depend>
    <test_depend>ros_testing</test_depend>
    <test_depend>launch</test_depend>
    <test_depend>launch_ros</test_depend>
    <test_depend>ament_cmake_gmock</test_depend>
    <test_depend>ament_index_python</test_depend>

    <export><build_type>ament_cmake</build_type></export>
</package>
