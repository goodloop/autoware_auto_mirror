<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format2.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
  <name>mpc_controller</name>
  <version>1.0.0</version>
  <description>A simple linear tire vehicle model mpc controller</description>
  <maintainer email="christopherj.ho@gmail.com">Christopher Ho</maintainer>
  <license>Apache License 2.0</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>
  <buildtool_depend>autoware_auto_cmake</buildtool_depend>

  <buildtool_export_depend>autoware_auto_cmake</buildtool_export_depend>

  <depend>autoware_auto_planning_msgs</depend>
  <depend>autoware_auto_vehicle_msgs</depend>
  <depend>controller_common</depend>
  <depend>motion_common</depend>

  <build_depend>acado_vendor</build_depend>
  <build_depend>motion_testing</build_depend>
  <build_depend>time_utils</build_depend>

  <test_depend>apex_test_tools</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export><build_type>ament_cmake</build_type></export>
</package>
